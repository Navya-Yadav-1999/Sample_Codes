@model IEnumerable<Employee>;

	<a class="btn btn-primary" asp-controller="Employee" asp-action="Create">Add New Employee</a>
<table class="border-2">
	<thead>
		<tr>
			<td>EmployeeId</td>
			<td>Name</td>
			<td>Salary</td>
			<td>Phone</td>
		</tr>
	</thead>
	@foreach (var employee in Model)
	{
			<tr>
				<td>@employee.EmployeeId</td>
				<td>@employee.Name</td>
				<td>@employee.Salary</td>
				<td>@employee.Phone</td>
			</tr>
	}
</table>



